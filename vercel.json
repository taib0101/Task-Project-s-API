{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "vercel-sapper"
        }
    ],
    "routes": [
        {
            "src": "/TaskProject/Login",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/Registration",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/ProfileDetails",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/CreateTask",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/ReadTask",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/UpdateTask",
            "dest": "/app.js"
        },
        {
            "src": "/TaskProject/DeleteTask",
            "dest": "/app.js"
        }
    ]
}